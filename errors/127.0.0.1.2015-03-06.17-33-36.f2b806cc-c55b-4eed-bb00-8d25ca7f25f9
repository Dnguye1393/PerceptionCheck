S'# -*- coding: utf-8 -*-\n# this file is released under public domain and you can use without limitations\n@auth.requires_login()\ndef index():\n    """Better index."""\n    # Let\'s get all data.\n    show_all = request.args(0) == \'all\'\n    q = (db.games) if show_all else (db.games.looking_for_players == True)\n\n    def generate_del_button(row):\n        # If the record is ours, we can delete it.\n        b = \'\'\n        if auth.user_id == row.user_id:\n            b = A(\'Delete\', _class=\'btn\', _href=URL(\'games\', \'delete\', args=[row.id], user_signature=True))\n        return b\n\n    def generate_edit_button(row):\n        # If the record is ours, we can delete it.\n        b = \'\'\n        if auth.user_id == row.user_id:\n            b = A(\'Edit\', _class=\'btn\', _href=URL(\'games\', \'edit\', args=[row.id]))\n        return b \n\n    def shorten_post(row):\n        return row.description[:10] + \'...\'\n\n    button = \'\'\n    if show_all:\n        button = A(\'See unsold\', _class=\'btn\', _href=URL(\'games\', \'index\'))\n    else:\n        button = A(\'See all\', _class=\'btn\', _href=URL(\'games\', \'index\', args=[\'all\']))\n\n    # Creates extra buttons.\n\n    links = [\n        dict(header=\'\', body = generate_del_button),\n        dict(header=\'\', body = generate_edit_button),\n        ]\n\n    start_idx = 1 if show_all else 0\n    form = SQLFORM.grid(q,\n        args=request.args[:start_idx],\n        fields=[db.games.game, db.games.edition, db.games.date_posted,\n                db.games.campaign_title,  db.games.meeting_location, db.games.meet_date, \n\t\t\t\tdb.games.welcome_new, db.games.looking_for_players, db.games.open_spots,\n                db.games.description],\n        editable=True, deletable=False,\n        links=links,\n        csv=False,\n        details=True,\n        searchable=False,\n        create=(auth.user_id is not None),\n        #paginate=10,\n        )\n    return dict(form=form, button=button)\n\n@auth.requires_login()\ndef delete():\n    """Deletes a post."""\n    p = db.games(request.args(0)) or redirect(URL(\'games\', \'index\'))\n    if p.user_id != auth.user_id:\n        session.flash = T(\'Not authorized.\')\n        redirect(URL(\'games\', \'index\'))\n    form = FORM.confirm(\'Are you certain?\')\n    if form.accepted:\n        db(db.games.id == p.id).delete()\n        redirect(URL(\'games\', \'index\'))\n    return dict(form=form)\n\n@auth.requires_login()\ndef toggle_sold():\n    item = db.games(request.args(0)) or redirect(URL(\'games\', \'index\'))\n    item.update_record(sold = not item.sold)\n    redirect(URL(\'games\', \'index\', args=[\'all\']))\n    return dict(item=item)\n\n@auth.requires_login()\ndef add():\n    """Add a post."""\n    form = SQLFORM(db.games)\n    if form.process().accepted:\n        # Successful processing.\n        session.flash = T("inserted")\n        redirect(URL(\'games\', \'index\'))\n    return dict(form=form)\n\ndef view():\n    """View a post."""\n    # p = db(db.games.id == request.args(0)).select().first()\n    p = db.games(request.args(0)) or redirect(URL(\'games\', \'index\'))\n    form = SQLFORM(db.games, record=p, readonly=True)\n    # p.name would contain the name of the poster.\n    return dict(form=form)\n\n@auth.requires_login()\ndef edit():\n    """View a post."""\n    # p = db(db.games.id == request.args(0)).select().first()\n    p = db.games(request.args(0)) or redirect(URL(\'games\', \'index\'))\n    if p.user_id != auth.user_id:\n        session.flash = T(\'Not authorized.\')\n        redirect(URL(\'games\', \'index\'))\n    form = SQLFORM(db.games, record=p)\n    db.games.sold.writeable = True #Allow changing "sold" status in Edit() mode\n    if form.process().accepted:\n        session.flash = T(\'Updated\')\n        redirect(URL(\'games\', \'view\', args=[p.id]))\n    # p.name would contain the name of the poster.\n    return dict(form=form)\n\ndef user():\n    """\n    exposes:\n    http://..../[app]/